{
  "name": "rsclone-backend",
  "version": "1.0.0",
  "description": "Командный проект школы RSSchool.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src/**/*.ts*",
    "lint:fix": "eslint ./src/**/*.ts* --fix",
    "test": "ts-node ./lib/server.ts",
    "dev": "tsc && nodemon ./dist/server.js",
    "cli:dev": "nodemon lib/server.ts"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "rules": {
      "linebreak-style": 0
    }
  },
  "eslintIgnore": [
    "*.test.*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kornienko199004/rsclone-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kornienko199004/rsclone-backend/issues"
  },
  "homepage": "https://github.com/kornienko199004/rsclone-backend#readme",
  "dependencies": {
    "@types/mongoose": "^5.10.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongodb": "^3.1.10",
    "mongoose": "^5.11.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.19",
    "@types/validator": "^13.1.2",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6",
    "validator": "^13.5.2",
    "validator.ts": "^0.2.2"
  }
}
